
<div>
      <h1>user signup</h1>
       <%= form_tag("/users/#{@user.id}/team/create") do %>
       
        <h2>group name</h2>
          <div class="cp_iptxt">
           <input type="text" name="name" value="<%= @group.team_name %>">
           <i class="fa fa-user fa-lg fa-fw" aria-hidden="true"></i>
         </div>

        <h2>guest name</h2>
          <div class="cp_iptxt">
           <input type="text" name="guest">
           <i class="fa fa-user fa-lg fa-fw" aria-hidden="true"></i>
         </div>


         <input type="submit">
        <% end %>

    </div>
   