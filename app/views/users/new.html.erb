<% provide(:title, 'Sign up') %>    
    
    <div>
      <h1>user signup</h1>
       <%= form_tag("/users/create",{multipart: true}) do %>
        <h2>user name</h2>
         <input type="text" name="name">
        <h2>user email</h2>
         <input type="text" name="email">
        <h2>user password</h2>
         <input type="password" name="password">
         <h2>image</h2>
         <input name="image" type="file">
         <input type="submit">
        <% end %>

    </div>
   
